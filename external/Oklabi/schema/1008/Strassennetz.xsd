                    <element name="in_Strassenknoten" 
                        type="okstra:ObjectRefType" minOccurs="0" maxOccurs="unbounded">
                        <annotation>
                            <appinfo>
                                <okstra:Zielobjekttyp>Strasse_Netzknoten</okstra:Zielobjekttyp>
                                <okstra:inverseRelation>hat_Netzknoten</okstra:inverseRelation>
                            </appinfo>
