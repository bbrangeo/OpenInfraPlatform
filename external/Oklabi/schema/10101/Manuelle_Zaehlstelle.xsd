                        type="okstra:ObjectRefType">
                        <annotation>
                            <appinfo>
                                <okstra:Zielobjekttyp>oertliche_Zuordnung_MZ</okstra:Zielobjekttyp>
                                <okstra:inverseRelation>von_manueller_Zaehlstelle</okstra:inverseRelation>
                            </appinfo>
