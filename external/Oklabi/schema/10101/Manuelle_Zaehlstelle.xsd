    <complexType name="raeuml_Gueltigkeitsbereich_MZType">
        <complexContent>
            <extension base="okstra:StreckenobjektHistType">
                <sequence>
                    <!-- Eigenschaften raeuml_Gueltigkeitsbereich_MZ -->
                    <element name="verbale_Beschreibung" 
